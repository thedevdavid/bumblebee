{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "globalEnv": [
    "NODE_ENV",

    "NEXT_PUBLIC_SITE_URL",
    "NEXT_PUBLIC_ROOT_DOMAIN",
    "PROJECT_ID_VERCEL",
    "NEXT_PUBLIC_SUPABASE_URL",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY",
    "SUPABASE_PROJECT_ID",
    "TRACKS_STORAGE_BUCKET",
    "CONTRACTS_STORAGE_BUCKET",
    "RELEASE_AGREEMENTS_STORAGE_BUCKET"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "@bumblebee/types#generate": {
      "dependsOn": ["@bumblebee/supabase#start", "^generate"],
      "outputs": []
    },
    "type-check": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "update-dependencies": {
      "cache": false
    }
  }
}
